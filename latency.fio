; -- start job file --
[read_throughput]

# Change values in this section for your run
# location where "Actual disk files" .Benchmark will run against these
directory=/home/user/fiotest

# latency related parameters
rw=randrw
rwmixread=70

# Other Parameters
iodepth=2
ioengine=libaio
bs=4k
direct=1
size=10G
numjobs=10
runtime=60s
ramp_time=2s
verify=0
group_reporting=1
; -- end job file --
